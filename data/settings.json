{
  "zones": {
    "autoReload": true,
    "validateBeforeReload": false,
    "backupEnabled": false,
    "autoGeneratePTR": true
  },
  "bind": {
    "version": "Bind 9.18.28",
    "configPath": "/etc/bind",
    "zonesPath": "/etc/bind/zones",
    "namedConfLocal": "/etc/bind/named.conf.local",
    "namedConfOptions": "/etc/bind/named.conf.options"
  },
  "resolver": {
    "enabled": true,
    "forwarders": [
      "8.8.8.8",
      "1.1.1.1",
      "94.140.14.14",
      "94.140.15.15"
    ],
    "queryLogging": true,
    "cacheSize": "256M",
    "dnssecValidation": true,
    "adblock": {
      "enabled": true,
      "blocklistUrls": [
        "https://raw.githubusercontent.com/StevenBlack/hosts/master/hosts",
        "https://raw.githubusercontent.com/hagezi/dns-blocklists/main/rpz/pro.txt",
        "https://raw.githubusercontent.com/hagezi/dns-blocklists/main/rpz/pro.txt",
        "https://raw.githubusercontent.com/hagezi/dns-blocklists/main/rpz/tif.txt"
      ],
      "redirectTo": "0.0.0.0",
      "customDomains": [],
      "wildcardEnabled": false,
      "wildcardDomains": []
    },
    "doh": {
      "enabled": true,
      "port": 443,
      "certPath": "/etc/ssl/certs/ndash.crt",
      "keyPath": "/etc/ssl/private/ndash.key"
    },
    "dot": {
      "enabled": true,
      "port": 853,
      "certPath": "/etc/ssl/certs/ndash.crt",
      "keyPath": "/etc/ssl/private/ndash.key"
    },
    "views": [
      {
        "name": "internal2",
        "acl": {
          "allow": [
            "10.0.0.0/8",
            "192.168.0.0/16"
          ],
          "deny": []
        },
        "zones": [
          "test3.example.com"
        ]
      },
      {
        "name": "internal6",
        "acl": {
          "allow": [
            "any"
          ],
          "deny": []
        },
        "zones": [
          "test2.example.com"
        ]
      },
      {
        "name": "global",
        "acl": {
          "allow": [
            "any"
          ],
          "deny": []
        },
        "zones": []
      }
    ]
  }
}